# Edit this file quickly and reload
alias newcom="vi $DOTFILES/.zsh_aliases && exec zsh"

# common
alias ll="ls -la"
alias c="clear"

# git
alias feh="git fetch --prune"
alias st="git status"
alias com="git commit -m"
alias pull="git pull"
alias chec="git checkout"
alias push="git push"
alias br="git branch -a"
alias add="git add"
alias glog="git log --reverse"
